     ]
    }
   ],
   "source": [
    "x = range(1000)\n",
    "\n",
    "def pure_sum():\n",
    "    return sum(x)\n",
    "\n",
    "def numpy_sum():\n",
    "    return np.sum(x)\n",
    "\n",
    "n = 10000\n",
    "\n",
    "t1 = timeit.timeit(pure_sum, number=n)\n",
    "print('pure python sum', t1)\n",
    "t2 = timeit.timeit(numpy_sum, number=n)\n",
    "print('numpy sum', t2)"
   ]
